{
  "name": "lgop",
  "version": "0.0.1",
  "description": "Map viewer",
  "scripts": {
    "build": "npx webpack --mode=production --config config/webpack.prod.js",
    "start": "npx webpack-dev-server --mode=development --config config/webpack.dev.js --open",
    "test": "nodeunit test",
    "watch": "npx webpack --mode=development --config config/webpack.dev.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/lgop"
  },
  "author": "stephen.d.francis@gmail.com",
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/__tests__/.*\\.(ts|tsx|js)$",
    "testEnvironmentOptions": {
      "url": "https://lgop/"
    }
  },
  "devDependencies": {
    "@types/loglevel": "^1.5.3",
    "@types/node": "^9.4.6",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/underscore": "^1.8.8",
    "@types/uuid": "^3.4.3",
    "jest": "^23.4.0",
    "lapis": "https://github.com/stephenfrancis/lapis.git",
    "loglevel": "^1.6.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.2",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.3",
    "underscore": "1.8.3",
    "uuid": "^3.2.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@types/jest": "^23.3.1"
  }
}
